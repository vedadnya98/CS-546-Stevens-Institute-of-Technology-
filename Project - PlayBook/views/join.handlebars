<div class="container">
    <ul class="collection with-header">
        <li class="collection-header">
            <h1>My Events</h1>
        </li>

        <form method="POST" action="/join">
            {{#each y}}
            <li class="collection-item">
                <p><span>Category:</span> {{this.sport}}</p>
                <p>Date: {{this.date}}</p>
                <p>Location: {{this.location}}</p>

                {{#each this.playersList}}
                <p style="font-size:1.5rem;">Player Lists : </h2>
                    <p>Email id: {{this.email}}</p>
                    {{!-- <p>{{this.cid}}</p> --}}


                    {{!-- <input type="text" name="eventId" hidden value="{{this.cid}}" /> --}}

                    <button class="btn waves-effect waves-light" type="submit" name="submit" value={{this.email}}>Remove
                        <i class="material-icons right">send</i>
                    </button>

                    {{/each}}

                    {{/each}}
            </li>
        </form>

    </ul>
</div>
